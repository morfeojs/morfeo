  "private": false,
  "version": "0.9.2",
  "license": "MIT",
  "main": "commonjs/index.js",
  "module": "build/index.js",
  "types": "build/index",
  "typings": "build/index",
  "keywords": [
    "morfeo",
    "morfeo-js",
    "build-time",
    "cli",
    "css-in-js",
    "css-in-ts"
