{
  "name": "@morfeo/utils",
  "author": {
    "name": "Mauro Erta",
    "email": "mauro@vlkstudio.com"
  },
  "private": false,
  "version": "0.9.2",
  "license": "MIT",
  "main": "commonjs/index.js",
  "module": "build/index.js",
  "types": "build/index",
  "typings": "build/index",
  "exports": {
    ".": {
      "import": {
        "types": "./build/index.d.ts",
        "default": "./build/index.js"
      },
      "require": {
        "types": "./commonjs/index.d.ts",
        "default": "./commonjs/index.js"
      }
    }
  },
  "keywords": [
    "design",
    "system",
    "morfeo",
    "morfeo-js",
    "utils"
  ],
  "scripts": {
    "build": "pnpm build:esm && pnpm build:commonjs",
    "build:esm": "rimraf build && tsc",
    "build:commonjs": "rimraf commonjs && tsc --module CommonJS --outdir commonjs",
    "watch": "tsc -w"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "build",
    "commonjs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/morfeojs/morfeo",
    "directory": "packages/utils"
  },
  "homepage": "https://morfeo.dev",
  "bugs": {
    "url": "https://github.com/morfeojs/morfeo/issues"
  }
}
